{% extends 'base.html.twig' %}

{% block title %}Edit Categorie{% endblock %}

{% block body %}
<div class="breadcrumbs">
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6 offset-lg-3 col-md-12 col-12">
                <div class="breadcrumbs-content">
                    <h1 class="page-title">Categorie manager</h1>
                    <ul class="breadcrumb-nav">
                        <li>
                            <a href="{{ path('home') }}">Home</a>
                        </li>
                        <li>
                            <a href="{{ path('categorie_index') }}">categorie list</a>
                        </li>
                        <li>Edit categorie</li>
                    </ul>
                </div>
            </div>
        </div>
    </div>
</div>

<div class="block-content mb-3">
    <div class="container">
        <div class="row">
            <div class="section-title">
                <h3 class="wow zoomIn" data-wow-delay=".2s" style="visibility:visible; animation-delay:0.2s;animation-name:zoomIn;">categorie Form</h3>
                <h2 class="wow fadeInUp" data-wow-delay=".4s" style="visibility:visible; animation-delay:0.4s;animation-name:fadeInUp;">Edit categorie</h2>
                
            </div>
        </div>
        <div class="row">
            <div class="form-group">
                {{ include('categorie/_form.html.twig', {'button_label': 'Update'}) }}
                {{ include('categorie/_delete_form.html.twig') }}

                <a href="{{ path('categorie_index') }}" class="btn btn-outline-secondary mt-3">back to list</a>

            </div>
        </div>
    </div>
</div>


    
{% endblock %}
